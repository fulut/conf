serv: sshd port 443

#!/bin/ksh
ssh -ND 1080 user@192.168.24.101  #./ff

ssh -vND 1080 -p 443 user@192.168.24.101
ssh -vND 0.0.0.0:1080 user@192.168.24.101 

scp root@123.123.123.123:/home/test.txt /directory


//
Host git.corp.example.com
  ProxyCommand=nc -X 5 -x localhost:1080 %h %p
////

cd nfq ./dvtws --port=989 --dpi-desync=fake,multisplit --dpi-desync-split-seqovl=681 --dpi-desync-split-pos=1 --dpi-desync-fooling=ts --dpi-desync-repeats=8 
privoxy
listen-address  127.0.0.1:8118
forward-socks5t   /               205.209.113.242:1080:1080 .

////

yt-dlp  
http-proxy=127.0.0.1:8118 http-proxy=socks5://205.209.113.242:1080

////
browser.cache.disk.enable false
/etc/sysctl.conf   sysctl     kern.bufcachepercent=50
https://p.thenewone.lol:8443/proxy.pac 
